.index_swiper {
  height: 340rpx;
}

.index_swiper .index_swiper_item image {
  width: 100%;
  height: 340rpx;
}

.index_category {
  display: flex;
  justify-content: space-around;
}

.index_category navigator {
  padding: 20rpx 0;
  flex: 1;
  text-align: center;
}

.index_category navigator image {
  width: 140rpx;
  height: auto;
}

.index_floor .floor-section {
  overflow: hidden;
}

.index_floor .floor-title {
  margin-top: 20rpx;
}

.index_floor .floor_items navigator {
  width: 33.33vw;
  height: 27.72711207vw;
  float: left;
  padding-left: 10rpx;
}

.index_floor .floor_items navigator:first-child {
  padding-left: 0;
  height: 55.45422414vw;
}

.index_floor .floor_items navigator:nth-child(2),
.index_floor .floor_items navigator:nth-child(3) {
  padding-bottom: 10rpx;
}

.index_floor .floor_items navigator image {
  height: 100%;
  width: 100%;
}